<mvc:View controllerName="logaligroup.applists.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content >
        <IconTabBar >
        <items>
         <IconTabFilter text="{i18n>objecList}" >
        <List items="{/Products}" >
         <items>
         <ObjectListItem title="{Material}   {Brand} {Model}"
           number="{Price}" numberUnit="{Currency}" type="Detail">
           <ObjectAttribute text="{Brand} "></ObjectAttribute>
           <ObjectAttribute text="{Weight}"></ObjectAttribute>
           </ObjectListItem>
         </items>
        </List>

        </IconTabFilter>
        
          <IconTabFilter text="{i18n>standardList}" >
         <List items="{/Products}" >
         <items>
         <StandardListItem title="{Material}   {Brand} {Model}" description="{Weight}" info="{Price} {Currency}" type="Active" >


         </StandardListItem>
         </items>
        </List>
        </IconTabFilter>

         <IconTabFilter text="{i18n>notificationList}">
        </IconTabFilter>

         <IconTabFilter text="{i18n>customList}">
        </IconTabFilter>
        
        </items>
        </IconTabBar>


        </content >
    </Page>
</mvc:View>
